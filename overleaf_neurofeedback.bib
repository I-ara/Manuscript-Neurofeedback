  author = {Ihssen, Niklas and Sokunbi, Moses O. and Lawrence, Andrew D. and Lawrence, Natalia S. and Linden, David E. J.},
  date = {2017-06},
  journaltitle = {Brain Imaging and Behavior},
  shortjournal = {Brain Imaging Behav},
  volume = {11},
