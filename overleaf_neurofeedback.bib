}

@article{ranceNeurofeedbackObsessiveCompulsive2023,
  title = {Neurofeedback for Obsessive Compulsive Disorder: {{A}} Randomized, Double-Blind Trial},
  shorttitle = {Neurofeedback for Obsessive Compulsive Disorder},
  author = {Rance, Mariela and Zhao, Zhiying and Zaboski, Brian and Kichuk, Stephen A. and Romaker, Emma and Koller, William N. and Walsh, Christopher and Harris-Starling, Cheyenne and Wasylink, Suzanne and Adams, Thomas and Gruner, Patricia and Pittenger, Christopher and Hampson, Michelle},
  date = {2023-10-01},
  journaltitle = {Psychiatry Research},
  shortjournal = {Psychiatry Research},
