  author = {Imperatori, Claudio and Valenti, Enrico Maria and Della Marca, Giacomo and Amoroso, Noemi and Massullo, Chiara and Carbone, Giuseppe Alessio and Maestoso, Giulia and Quintiliani, Maria Isabella and Contardi, Anna and Farina, Benedetto},
  date = {2017-02-01},
  journaltitle = {International Journal of Psychophysiology},
